{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eslint no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }],
        "eslint import/newline-after-import": "off",
        "linebreak-style": 0 ,
        "eslint no-console": "allow",
        "parser": "babel-eslint"
       
    }
}
